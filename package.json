{
  "name": "obsiscribe",
  "version": "1.0.0",
  "description": "Obsidian plugin for speech-to-text conversion using Moonshine AI",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs development",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup": "node scripts/download-models.js",
    "postinstall": "npm run setup",
    "release": "node scripts/prepare-release.js",
    "prerelease": "npm run build && npm test"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "speech-to-text",
    "transcription",
    "moonshine-ai"
  ],
  "author": "Sairam Koripelly <sairamk0@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "8.18.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "obsidian": "latest",
    "ts-jest": "^29.1.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "mic": "^2.1.2",
    "sherpa-onnx-darwin-arm64": "^1.12.9",
    "sherpa-onnx-node": "^1.12.9"
  }
}
